<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_aarti_menu"
    tools:context="com.example.user.dhumilyadein.AartiMenu">

    <TextView android:text="@string/aarti_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginLeft="100dp"
        android:textSize="30sp"
        android:textColor="#6600ff"
        android:id="@+id/textView2" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Genesh ji aarti"
        android:id="@+id/ganeshjiaartiBtn"
        android:layout_marginTop="60dp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Ambe ji aarti"
        android:id="@+id/ambejiaartiBtn"
        android:layout_marginTop="120dp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Genesh ji ki aarti"
        android:id="@+id/ganeshjiaartiTxtBtn"
        android:layout_marginTop="180dp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Jai Jagdish Hare aarti"
        android:id="@+id/jagdishHareAartiTxtBtn"
        android:layout_marginTop="240dp" />

</RelativeLayout>
